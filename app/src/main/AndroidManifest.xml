<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zad2_v2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Zad2_v2">
        <activity android:name=".ViewActivity"></activity>
        <activity
            android:name=".MainActivity2"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.Zad2_v2.NoActionBar"></activity>
        <activity
            android:name=".ActivityInfo"
            android:theme="@style/Theme.Zad2_v2.NoActionBar" />
        <activity
            android:name=".ActivityHobby"
            android:theme="@style/Theme.Zad2_v2.NoActionBar" />
        <activity
            android:name=".ActivityGrid"
            android:theme="@style/Theme.Zad2_v2.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>